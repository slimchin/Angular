   
    @{
        if (ViewBag.ConsentDetail != null)
        {

            <div class="row" style="padding-top:10px">
                <div class="col-2" style="text-align:right">
                    <h5>Consent Form ID</h5>
                </div>
                <span >
                    @ViewBag.ConsentDetail.consentFormID
                    <input type="hidden" name="consentFormIDStr" id="consentFormIDStr" value="@ViewBag.ConsentDetail.consentFormID" />
                    <input type="hidden" name="consentIDStr" id="consentIDStr" value="@ViewBag.consentIDStr" />
                </span>
            </div>
            foreach (var m in ViewBag.ConsentDetail.consent)
            {

                <div class="row" style="padding-top:10px" >
                    <div class="col-2" style="text-align:right">
                        <h5>@*@m.consentOrder*@</h5>
                            

                    </div>
                    <div class="col-10">
                        
                        <span >
                            <input type="checkbox" name="Order" value="@m.consentID" id="Order"> 
                            @m.contentText
                        </span>
                        
                    </div>
                </div>
            }
        }
    }
    
